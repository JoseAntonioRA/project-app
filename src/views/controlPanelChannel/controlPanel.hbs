<div "containerMain">
	{{#each channel}}
	<div class="panelControl">
		<form class="formularioo" id="formPanelControl">
			<input type="hidden" name="__method" value="PUT">
			<div class="form-group">
				<label for="title">Título</label>
				<input type="search" name="title" id="title" value="{{title}}">
			</div>
			
			<div class="form-group">
				<label for="category">Categoría</label>
				<i class="icono izquierda fa fa-search"></i><input data-search type="search" id="category" autocomplete="off" value="{{category}}">
			</div>
			<br><br>
			<input type="submit" value="Actualizar información" id="btn-update">
		</form>
		<form class="" id="formLive">
			<input type="hidden" name="method" value="true">
			<input type="submit" value="Iniciar Stream" id="">
		</form>
	</div>
	{{/each}}
	<div class="contentNano">
		<div id="about" class="nano">
			<div class="content" id="content">
				{{#each categories}}
				<button data-filter-item data-filter-name="{{name}}" value="{{name}}" type="submit"
					class="btn-category">{{name}}</button>
				{{/each}}
			</div>
		</div>
	</div>
</div>